<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!--site css-->
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="icon" href="/asset/image/favicon.ico">

    <!--Quasar-->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900|Material+Icons" rel="stylesheet"
        type="text/css">
    <link href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css"
        rel="stylesheet" type="text/css">
    <link href="https://cdn.jsdelivr.net/npm/quasar@2.11.2/dist/quasar.css" rel="stylesheet" type="text/css">


</head>

<body style="background-image: url('https://wallpapercave.com/wp/wp3631186.jpg'); background-size: cover; background-position: center; background-repeat: no-repeat;">
    @RenderBody()

    <!--site js-->
    <script src="~/js/site.js" asp-append-version="true"></script>


    <!--quasar cdn-->
    <!-- Add the following at the end of your body tag -->
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue@3/dist/vue.global.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/quasar@2.11.2/dist/quasar.umd.js"></script>

    <!-- chart js -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    @RenderSection("code", required: false)
</body>


</html>